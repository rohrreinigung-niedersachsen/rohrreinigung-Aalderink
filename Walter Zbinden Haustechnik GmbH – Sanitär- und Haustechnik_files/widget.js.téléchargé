function applyMasonry(sk_instagram_feed){$grid=new Masonry('.sk-ig-all-posts',{itemSelector:'.sk-instagram-feed-item',columnWidth:'.sk-instagram-feed-item-sizer',percentPosition:true,transitionDuration:0});}
